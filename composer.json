{
  "name": "jamesblackwell/artisan-ai",
  "description": "A Laravel package for scaffolding Laravel files using OpenAI's GPT-4",
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/http": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "JamesBlackwell\\ArtisanAI\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JamesBlackwell\\ArtisanAI\\ArtisanAIServiceProvider"
      ]
    }
  }
}
